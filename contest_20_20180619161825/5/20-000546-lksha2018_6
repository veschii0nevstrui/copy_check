# include <iostream>
using namespace std;
int main(){
int n,n2,n1,n3,S;
cin>>n;
n1=n/100;
n2=n/10-n1*10;
n3=n-(n1*100+n2*10);
if(n2!=0&&n3!=0){
S=3*2*1;
 if(n1==n2){
S=S/2+(S%2!=0);
 }
    if(n1==n3){
          S=S/2+(S%2!=0);
    }
        if(n2==n3){
       S=S/2+(S%2!=0);
        }
 	cout<<S;
}
else{if(n2==0&&n3==0){
	cout<<1;
	return 0;
}
	S=2*2*1;
    if(n1==n2){
   S=S/2+(S%2!=0);
    }
    if(n1==n3){
          S=S/2+(S%2!=0);
    }
        if(n2==n3){
        S=S/2+(S%2!=0);
        }
cout<<S;
}
return 0;
}