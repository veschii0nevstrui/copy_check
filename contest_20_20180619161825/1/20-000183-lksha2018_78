#include <iostream>

using namespace std;

int main () {
   int a,b,c;
   cin>>a>>b>>c;
   if(a==b&&a==c) {
    cout<<a;
    return 0;
   }
   if(a==b||a==c||b==c) {
        if(a==b) {
            cout<<a;
        }
        if(a==c) {
            cout<<a;
        }
        if(b==c) {
            cout<<b;
        }
   }
   else {
    if(a>=b&&a>=c) {
        cout<<a;
   }
    if(b>=a&&b>=c) {
        cout<<b;
   }
    if(c>=a&&c>=b) {
        cout<<c;
   }
   }
}
