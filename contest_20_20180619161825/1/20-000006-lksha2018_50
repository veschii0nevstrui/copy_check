#include <iostream>

using namespace std;

int main(){

int c1, c2, c3;

cin >>c1 >>c2 >>c3;
int a, b, c, d, e;

if ((c1 >= c2) && (c1 >= c3)){
    cout <<c1;
    return 0;
}if ((c2 >= c3) && (c2 >= c1)){
    cout <<c2;
    return 0;
    }else{
    cout <<c3;
    return 0;
    }
}
