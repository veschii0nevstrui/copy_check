#include <iostream>
using namespace std;
int main () {
int a, b, c, y = 10;
cin >> a >> b >> c;
if  (a >= b && a >=c) {
    cout << a;
    } else {

    if (b >= a && b >= c) {
        cout << b;
}
else {

        if  (c >= a && c >= b) {
            cout << c;
}
}
    }
    return 0;
}
