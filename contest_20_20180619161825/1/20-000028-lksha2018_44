#include <iostream>

using namespace std;

int main () {

int a, b, c;

cin>>a>>b>>c;

if (a>=b&&a>=c) {

    cout<<a;

    return 0;
}

else {

    if (b>=a&&b>=c) {

        cout<<b;

        return 0;

    }

    else {

        if (c>=a&&c>=b) {

            cout<<c;

            return 0;
        }
    }
}
}
