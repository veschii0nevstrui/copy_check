#include <iostream>
using namespace std;

int main() {
	int a, b, c;
	cin >> a >> b >> c;
	if (a>b &&a>c) {
		cout<<a;
	}
	if (b>a && b>c) {
		cout<<b;
	}
	if (c>a && c>b) {
		cout<<c;
	}
	if (a==b && b==c) {
		cout<<b;
	}
	if (c==b && c>a) {
		cout<<c;
	}
	if (a==c && a>b) {
		cout<<a;
	}
	if (a==b && a>c) {
		cout<<a;
	}
	return 0;
}